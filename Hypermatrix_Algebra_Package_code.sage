        sage: Bool2Poly2(['AND', ['OR', var('x0'), ['OR', var('x0'), 1]], ['AND', ['AND', 1, var('x1')], 1]],[var('x0') ,var('x1')])
        -(x0 - 1)*x1 + x0*x1


