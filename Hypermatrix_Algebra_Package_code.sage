        while A.slice(rg(i,A.n(0)),'row').slice([j],'col').is_zero() and j < A.n(1)-1:
            # Incrementing the column index
            j=j+1
        if A.slice(rg(i,A.n(0)),'row').is_zero()==False:
            while A[i,j].is_zero(): 
                Ta=A.slice(rg(i,A.n(0)),'row')
                Tb=b.slice(rg(i,b.n(0)),'row')
