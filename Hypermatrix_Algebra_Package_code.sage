        else :
            while (A[i,j]).is_zero():
                # Incrementing the column index
                j = j + 1
            # performing row operations
            cf1=A[i,j]
            for r in range(i-1,-1,-1):
                #b[r,:] = -A[r,j]*b[i,:]+b[r,:]
                cf2=A[r,j]
                for j0 in range(b.n(1)):
                    b[r,j0]=cf2*b[i,j0]-cf1*b[r,j0]
                #A[r,:] = -A[r,j]*A[i,:]+A[r,:]
                for j0 in range(A.n(1)):
                    A[r,j0]=cf2*A[i,j0]-cf1*A[r,j0]
