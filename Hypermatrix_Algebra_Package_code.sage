        # Initialization of a buffer list Ng1
        # which will store updates to Ng0
        Ng1 = []
        for n in Set(Ng0).subsets():
            if n.cardinality() > 0:
                Ng1.append(sum(n))
        Ng0 = list(Ng1)
    Nf = []
    for i in range(len(Ng0)):
        Nf.append([])
    for i in range(len(Nf)):
        Nf[(Ng0[i]).subs(x=2)-1].append(Ng0[i])
