        Eq=[X[i,s,t]*Y[s,j,t]==B[i*sz1*sz2+j*sz2+t,i*sz1*sz2+j*sz2+s] for i in rg(sz0) for j in rg(sz1) for s in rg(sz2) for t in rg(sz2)]
