        if HM(1,A.n(1),[A[i,j0] for j0 in range(A.n(1))]).is_zero():
            # decrementing the row index and initializing the column index
            i=i-1; j=0
        else :
            while (A[i,j]).is_zero():
                # Incrementing the column index
                j = j + 1
