def EvalT(T):
    """
    Outputs the evaluation value of a tree.

    EXAMPLES:

    ::

        sage: EvalT(['+', ['+', 1, ['+', 1, 1]], ['+', ['+', 1, 1], 1]])
        6

    AUTHORS:
    - Edinah K. Gnang and Doron Zeilberger
    """
    if T == 1:
        return 1
    elif T == -1:
        return -1
    elif T[0] == '+':
        return EvalT(T[1]) + EvalT(T[2])
    elif T[0] == '*':
        return EvalT(T[1]) * EvalT(T[2])
    elif T[0] == '^':
        return EvalT(T[1]) ^ EvalT(T[2])
    else:
        print('IMPROPER INPUT !!!')

