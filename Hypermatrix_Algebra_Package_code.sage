    """
    if H.is_cubical(): 
        f=GeneralHyperdeterminantExpression(H.order(),H.n(0))
        Lh=H.list()
        #Lx=apply(HM,[H.n(0) for i in range(H.order())]+['x']+['shift']).list()
        Lx=HM(*([H.n(0) for i in range(H.order())]+['x']+['shift'])).list()
